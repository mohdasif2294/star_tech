{
  "name" : "BrowsingData API: Basics",
  "version" : "1.1",
  "description" : "A trivial usage example.",
  "permissions": [
    "browsingData",
    "tabs",
    "storage",
   "declarativeContent"
  ],
  "background": {
    "scripts": ["b.js"],
    "persistent": false
  },
"content_scripts": [{
      "matches": ["https://*.myntra.com/"],
      "js": ["content.js"]
  }],
  "browser_action": {
    "default_popup": "basic.html",
    "default_icon": "ronaldo.jpg"
  },
  "manifest_version": 2
}
